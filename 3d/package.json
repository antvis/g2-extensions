{
  "name": "@antv/g2-extension-3d",
  "version": "0.1.0",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "fix": "prettier --write src __tests__",
    "test": "npm run test:unit && npm run test:format",
    "test:unit": "vitest --watch=false",
    "test:format": "prettier --check src __tests__",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
    "build": "run-p build:*"
  },
  "dependencies": {
    "d3-array": "^3.2.2",
    "@antv/g-webgl": "^1.9.8",
    "@antv/g-plugin-3d": "^1.9.5",
    "@antv/g-plugin-control": "^1.9.5",
    "@antv/util": "^3.3.5"
  },
  "peerDependencies": {
    "@antv/g2": "^5.1.5"
  },
  "devDependencies": {
    "@types/d3-array": "3.0.5",
    "@antv/g2": "^5.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vitest": "^0.34.2"
  },
  "repository": "https://github.com/antvis/g2-extentions.git"
}
