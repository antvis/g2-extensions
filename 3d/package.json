{
  "name": "@antv/g2-extension-3d",
  "version": "0.1.1-beta.1",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/index.umd.min.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm",
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "fix": "prettier --write src __tests__",
    "test": "npm run test:unit && npm run test:format",
    "test:unit": "vitest --watch=false",
    "test:format": "prettier --check src __tests__",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
    "build:umd": "rimraf ./dist && rollup -c",
    "build": "run-p build:*"
  },
  "dependencies": {
    "d3-array": "^3.2.2",
    "@antv/g-webgl": "^1.9.8",
    "@antv/g-plugin-3d": "^1.9.5",
    "@antv/g-plugin-control": "^1.9.5",
    "@antv/util": "^3.3.5"
  },
  "peerDependencies": {
    "@antv/g2": "^5.1.8-beta.1"
  },
  "devDependencies": {
    "@types/d3-array": "3.0.5",
    "@antv/g2": "^5.1.8-beta.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.2",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vitest": "^0.34.2"
  },
  "repository": "https://github.com/antvis/g2-extensions.git"
}
