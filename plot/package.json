{
  "name": "@antv/g2-extension-plot",
  "version": "0.1.0",
  "description": "The mark plot in antv/g2",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/index.umd.min.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm",
    "dist"
  ],
  "sizeEffects": false,
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "fix": "prettier --write src __tests__",
    "test": "jest",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
    "build": "run-p build:*",
    "ci": "npm run test && npm run build"
  },
  "dependencies": {
    "@antv/g2": "^5.1.8",
    "@antv/g": "^5.18.19",
    "@antv/util": "^3.3.5",
    "@antv/g-svg": "^1.10.21",
    "@antv/g-webgl": "^1.9.29",
    "@antv/g-canvas": "^1.11.22",
    "@antv/g-plugin-3d": "^1.9.5",
    "@antv/g-plugin-control": "^1.9.17",
    "@antv/g-plugin-dragndrop": "^1.8.17",
    "@antv/g-plugin-rough-svg-renderer": "^1.9.22",
    "@antv/g-plugin-rough-canvas-renderer": "^1.9.22",
    "d3-array": "^3.2.4",
    "colormap": "^2.3.2",
    "ndarray": "^1.0.19",
    "ndarray-ops": "^1.2.2",
    "ndarray-pack": "^1.2.1",
    "d3-hierarchy": "^3.1.2",
    "ndarray-gradient": "^1.0.1"
  },
  "devDependencies": {
    "@antv/g-pattern": "^1.2.19",
    "@antv/g-webgl": "^1.9.8",
    "@antv/g-plugin-control": "^1.9.5",
    "@types/node": "^16.11.7",
    "@types/jest": "^29.0.0",
    "@types/d3-array": "3.0.5",
    "vite": "^4.3.9",
    "jest": "^29.0.0",
    "rimraf": "^5.0.5",
    "vitest": "^0.34.2",
    "ts-jest": "^29.1.0",
    "prettier": "^3.0.3",
    "pixelmatch": "5.3.0",
    "typescript": "^4.9.5",
    "npm-run-all": "^4.1.5",
    "jest-environment-node": "^29.7.0"
  },
  "repository": "https://github.com/antvis/g2-extensions.git"
}